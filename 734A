#include<bits/stdc++.h>
using namespace std;
 
int main(){
    int n;
    cin>>n;
    string a;
    cin>>a;
    int countd=0;
    int counta=0;
    for(int i=0;i<n; i++){
        if(a[i]==65){
            counta++;
        }
    }
    for(int i=0;i<n; i++){
        if(a[i]==68){
            countd++;
        }
    }
    if(counta>countd){
        cout<<"Anton"<<endl;
    }
    if(countd>counta){
        cout<<"Danik"<<endl;
    }
    if(countd==counta){
        cout<<"Friendship"<<endl;
    }
    return 0;
}
